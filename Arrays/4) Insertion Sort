// INSERION SORT
const insertionSort = (array) => {
  for (i = 1; i < array.length; i++)
  {
    let j = i
    while (j > 0 && array[j] < array[j - 1])
    console.log(array[j])
    {
      [array[j], array[j - 1]] = [array[j - 1], array[j]]
      j--
      }
  }
  return array
};
console.log(insertionSort([10,2,3,5,31,1]));

